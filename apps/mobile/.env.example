# App Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_ENVIRONMENT=development

# Auth Configuration
EXPO_PUBLIC_COGNITO_USER_POOL_ID=us-east-1_xxxxxx
EXPO_PUBLIC_COGNITO_USER_POOL_WEB_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxx
EXPO_PUBLIC_COGNITO_IDENTITY_POOL_ID=us-east-1:xxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxx

# Google Auth
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=xxxxxx-xxxxxxxxxxxxxx.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=xxxxxx-xxxxxxxxxxxxxx.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=xxxxxx-xxxxxxxxxxxxxx.apps.googleusercontent.com

# Maps Configuration
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=xxxxxxxxxxxxxxxxxxxxxx

# Search Configuration
EXPO_PUBLIC_SEARCH_ENDPOINT=https://xxxxx.us-east-1.es.amazonaws.com
EXPO_PUBLIC_SEARCH_API_KEY=xxxxxxxxxxxxxxxxxxxxxx

# Feature Flags
EXPO_PUBLIC_ENABLE_CHAT=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true

# Currency Configuration
EXPO_PUBLIC_DEFAULT_CURRENCY=USD
EXPO_PUBLIC_SUPPORTED_CURRENCIES=USD,JMD,BSD
