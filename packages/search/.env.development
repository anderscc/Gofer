# Environment
NODE_ENV=development

# OpenSearch Configuration
OPENSEARCH_DOMAIN_ENDPOINT=http://localhost:9200
OPENSEARCH_MASTER_USER=admin
OPENSEARCH_MASTER_PASSWORD=admin
OPENSEARCH_INDEX_PREFIX=dev

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=gofer-dev

# VPC Configuration
VPC_ID=vpc-local
SUBNET_IDS=subnet-local
SECURITY_GROUP_ID=sg-local

# Performance & Resilience
MAX_CONNECTIONS=5
CONNECTION_TIMEOUT=3000
REQUEST_TIMEOUT=10000
RETRY_COUNT=2
BULK_SIZE=50

# Monitoring
ENABLE_DEBUG_LOGGING=true
APM_SERVICE_NAME=gofer-search-dev
TELEMETRY_ENABLED=false
